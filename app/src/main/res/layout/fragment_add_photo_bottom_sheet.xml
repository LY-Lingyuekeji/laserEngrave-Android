<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_white_ffffff_topradius30"
    android:orientation="vertical"
    android:paddingHorizontal="10dp"
    android:paddingVertical="20dp"
    tools:context=".fragment.WifiChooseBottomSheetFragment">


    <RelativeLayout
        android:id="@+id/rl_material"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_white_ffffff_r10"
        android:padding="10dp">

        <ImageView
            android:id="@+id/iv_engrave_list_item_logo"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:src="@drawable/ic_star" />


        <TextView
            android:id="@+id/iv_engrave_list_item_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="素材库"
            android:textColor="@color/colorBlack"
            android:textSize="16sp" />

    </RelativeLayout>



</LinearLayout>